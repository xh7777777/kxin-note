{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "main": "./dist/electron/main/main.js",
  "author": "sky <https://github.com/umbrella22>",
  "description": "electron-vite-template project",
  "license": "MIT",
  "scripts": {
    "dev": "tsx .electron-vite/dev-runner.ts",
    "build": "tsx .electron-vite/build.ts -m prod && electron-builder -c build.json",
    "build:win32": "tsx .electron-vite/build.ts -m prod  && electron-builder -c build.json --win  --ia32",
    "build:win64": "tsx .electron-vite/build.ts -m prod  && electron-builder -c build.json --win  --x64",
    "build:mac": "tsx .electron-vite/build.ts -m prod  && electron-builder -c build.json --mac",
    "build:dir": "tsx .electron-vite/build.ts -m prod  && electron-builder -c build.json --dir",
    "build:clean": "tsx .electron-vite/build.ts --clean",
    "build:web": "tsx .electron-vite/build.ts",
    "pack:resources": "tsx .electron-vite/hot-updater.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts,.vue,.js",
    "lint:fix": "eslint . --ext .ts,.vue,.js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.30.8",
    "@tailwindcss/postcss": "^4.1.8",
    "electron-log": "^5.3.0",
    "electron-updater": "^6.3.9",
    "lucide-vue-next": "^0.513.0",
    "semver": "^7.6.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "axios": "^1.7.8",
    "cfonts": "^3.3.0",
    "chalk": "5.4.1",
    "del": "^8.0.0",
    "dotenv": "^16.4.7",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "electron-devtools-vendor": "^3.0.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^10.2.0",
    "fs-extra": "^11.3.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^12.4.1",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jest-transform-stub": "^2.0.0",
    "listr2": "^8.2.5",
    "minimist": "^1.2.8",
    "pinia": "^2.3.1",
    "portfinder": "^1.0.32",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "rollup-plugin-esbuild": "^6.2.0",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.84.0",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.5.0"
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ]
}
